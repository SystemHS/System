function FWhiteFlash()
    char = game.Players.LocalPlayer.Character
    for _,v in pairs(rep.Resources.LocalScripts:GetChildren()) do
    if v.Name == 'ShortWhiteScreen2' then
        clone = v:Clone()
        clone.Parent = char
       end
    end
end
    

